{
    "name": "condenast-ru/basic-api-bundle",
    "description": "Basic API bundle",
    "type": "symfony-bundle",
    "keywords": ["API", "JSON"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zhurin Vasiliy",
            "email": "vzhurin@condenast.ru"
        }
    ],
    "require": {
        "php": "^7.3||^8",
        "doctrine/annotations": "^1.7",
        "symfony/config": "^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/error-handler": "^5.1",
        "symfony/event-dispatcher": "^5.1",
        "symfony/framework-bundle": "^5.1",
        "symfony/http-foundation": "^5.1",
        "symfony/http-kernel": "^5.1",
        "symfony/property-access": "^5.1",
        "symfony/serializer": "^5.1",
        "symfony/string": "^5.1",
        "symfony/validator": "^5.1"
    },
    "require-dev": {
        "ext-json": "*",
        "friendsofphp/php-cs-fixer": "^3",
        "nelmio/api-doc-bundle": "^4",
        "symfony/asset": "^5.1",
        "symfony/browser-kit": "^5.1",
        "symfony/console": "^5.1",
        "symfony/dotenv": "^5.1",
        "symfony/phpunit-bridge": "^5.1.3",
        "symfony/routing": "^5.1.4",
        "symfony/twig-bundle": "^5.1",
        "symfony/yaml": "^5.1",
        "vimeo/psalm": "^4.6"
    },
    "suggest": {
        "nelmio/api-doc-bundle": "To generate API documentation"
    },
    "conflict": {
        "doctrine/annotations": "1.10.3",
        "phpdocumentor/type-resolver": "1.0.0",
        "nelmio/api-doc-bundle": "<4"
    },
    "autoload": {
        "psr-4": {
            "Condenast\\BasicApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Condenast\\BasicApiBundle\\Tests\\Fixtures\\App\\": "tests/Fixtures/App/src/",
            "Condenast\\BasicApiBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
