{
    "name": "condenast-ru/basic-api-bundle",
    "description": "Basic API bundle",
    "type": "symfony-bundle",
    "keywords": ["API", "JSON"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zhurin Vasiliy",
            "email": "vzhurin@condenast.ru"
        }
    ],
    "require": {
        "php": "^7.2",
        "doctrine/annotations": "^1.5",
        "phpdocumentor/reflection-docblock": "^5",
        "symfony/config": "^5",
        "symfony/dependency-injection": "^5",
        "symfony/error-handler": "^5",
        "symfony/event-dispatcher": "^5",
        "symfony/framework-bundle": "^5",
        "symfony/http-foundation": "^5",
        "symfony/http-kernel": "^5",
        "symfony/property-access": "^5",
        "symfony/property-info": "^5",
        "symfony/serializer": "^5",
        "symfony/validator": "^5"
    },
    "require-dev": {
        "ext-json": "*",
        "dg/bypass-finals": "^1.1",
        "doctrine/cache": "^1.7",
        "friendsofphp/php-cs-fixer": "^2.15",
        "nelmio/api-doc-bundle": "^3",
        "phpstan/phpstan": "^0.12.19",
        "phpstan/phpstan-deprecation-rules": "^0.12.2",
        "ramsey/uuid": "^3|^4",
        "symfony/asset": "^5",
        "symfony/browser-kit": "^5",
        "symfony/console": "^5",
        "symfony/dotenv": "^5",
        "symfony/phpunit-bridge": "^5",
        "symfony/routing": "^5",
        "symfony/twig-bundle": "^5",
        "symfony/yaml": "^5"
    },
    "suggest": {
        "nelmio/api-doc-bundle": "To generate API documentation",
        "ramsey/uuid": "To deal with UUIDs"
    },
    "conflict": {
        "phpdocumentor/type-resolver": "1.0.0",
        "nelmio/api-doc-bundle": "<3",
        "ramsey/uuid": "<3"
    },
    "autoload": {
        "psr-4": {
            "Condenast\\BasicApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Condenast\\BasicApiBundle\\Tests\\Fixtures\\App\\": "tests/Fixtures/App/src/",
            "Condenast\\BasicApiBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
