{
    "name": "condenast-ru/basic-api-bundle",
    "description": "Basic API bundle",
    "type": "symfony-bundle",
    "keywords": ["API", "JSON"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zhurin Vasiliy",
            "email": "vzhurin@condenast.ru"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/config": "^6",
        "symfony/dependency-injection": "^6",
        "symfony/error-handler": "^6",
        "symfony/event-dispatcher": "^6",
        "symfony/framework-bundle": "^6",
        "symfony/http-foundation": "^6",
        "symfony/http-kernel": "^6",
        "symfony/property-access": "^6",
        "symfony/serializer": "^6",
        "symfony/string": "^6",
        "symfony/validator": "^6"
    },
    "require-dev": {
        "ext-json": "*",
        "friendsofphp/php-cs-fixer": "^3",
        "nelmio/api-doc-bundle": "^4",
        "symfony/asset": "^6",
        "symfony/browser-kit": "^6",
        "symfony/console": "^6",
        "symfony/dotenv": "^6",
        "symfony/phpunit-bridge": "^6",
        "symfony/routing": "^6",
        "symfony/runtime": "^6",
        "symfony/twig-bundle": "^6",
        "symfony/yaml": "^6",
        "vimeo/psalm": "^4.6"
    },
    "suggest": {
        "nelmio/api-doc-bundle": "To generate API documentation"
    },
    "conflict": {
        "nelmio/api-doc-bundle": "<4"
    },
    "autoload": {
        "psr-4": {
            "Condenast\\BasicApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Condenast\\BasicApiBundle\\Tests\\Fixtures\\App\\": "tests/Fixtures/App/src/",
            "Condenast\\BasicApiBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
